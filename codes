void setup()
{
 for (int i=9; i<=13; i++) 
 {
  pinMode(i, OUTPUT);
 }
}
void loop()
{
 for (int i=9; i<=13; i++) 
 {
  allLEDsOff();
  if (i!=13)
   {
    digitalWrite(i,HIGH);
    digitalWrite(i+1,HIGH);
    delay(200);
   }
   else
   {
    digitalWrite(i,HIGH);
    digitalWrite(i-3,HIGH);
    delay(200);
    allLEDsOff();
   }
 }
}

void allLEDsOff(void)
{
for (int i=9; i<=13; i++) 
 {
  digitalWrite(i, LOW);
 }
delay(100);
}
